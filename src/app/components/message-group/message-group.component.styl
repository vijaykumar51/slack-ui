.message-group
	padding: 10px 20px
	.group-info-container
		.date
			color: #666
			font-weight: bold
			display: flex
			align-items: center
			white-space: nowrap
			&:before,
			&:after
				content: ""
				border-top: 1px solid black
				width: 100%
			&:before
				margin-right: 10px
			&:after
				margin-left: 10px
